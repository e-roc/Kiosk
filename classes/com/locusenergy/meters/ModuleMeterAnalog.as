package com.locusenergy.meters {		import flash.display.*;	import caurina.transitions.Tweener;	import flash.text.*;	import flash.utils.Timer;	import flash.net.URLRequest;	import flash.events.*;			public class ModuleMeterAnalog extends AnalogMeter	{		public function ModuleMeterAnalog(reading:Number = 0.0, max:Number = 0.0, meterTitle:String = "INSTANTANEOUS POWER") { 						trace("NEW MODULE METER ANALOG");			this.max = max;			this.reading = reading;						counter = 0;			meterOutput = new MeterOutput(meterTitle, "kW", true);			meterOutput.x = 150;			meterOutput.y = 80;			this.addChild(meterOutput);			this.addChild(meter);			initialize();					}		override public function startMeter()		{			trace ("STARTING METERS");						animateMeter(reading,max);			meterOutput.updateMeter(reading, rot, time);		}		override public function setMeterReading(reading)		{			this.reading = reading;			if (this.reading >= max)	animateMeter(max,max)			else animateMeter(this.reading, max);			meterOutput.updateMeter(this.reading, rot, time);		}		override protected function removeAdditionalEventHandlers()		{			if (meterOutput != null)	meterOutput.cleanUp();		}	}	}