package com.locusenergy.meters {		public class ModuleMeterDigital extends DigitalMeter{				// Constants:		// Public Properties:		// Private Properties:			// Initialization:		public function ModuleMeterDigital(reading:Number = 0.0, meterTitle:String = "GENERATED TO DATE") 		{ 						meterOutput = new MeterOutput(meterTitle, "kWh", true);			meterOutput.x = 150;			meterOutput.y = 80;			//trace ("Dig Reading: " + reading);			this.reading = Number(reading.toFixed(2));			this.addChild(meterOutput);			this.addChild(meter);			initialize();		}		public function updateTitle(newTitle)		{			meterOutput.updateTitle(newTitle);		}		override public function startMeter()		{			textDisplay();			trace(reading, reps, time, meterOutput);			meterOutput.updateMeter(reading, reps, time);					}		override protected function removeAdditionalEventHandlers()		{			if (meterOutput != null)	meterOutput.cleanUp();		}		// Public Methods:		// Protected Methods:	}	}