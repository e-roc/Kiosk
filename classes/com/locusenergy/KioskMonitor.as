package com.locusenergy{	import flash.text.TextField;	import flash.display.Sprite;	import com.locusenergy.Connector;		public class KioskMonitor{		public static var componentName;		public static var functionName;		public static var fullscreenMode;		public static var url;		public static var kioskID;		public static var connector:Connector;				public function KioskMonitor() {			// constructor code		}		public static function reportStatus()		{			var kStatus;			if (fullscreenMode == null || fullscreenMode == "normal")				kStatus = 0;			else				kStatus = 1;			//connector.postData(completeFunc, new Array("abc", 123, false));			connector.postData(componentName + "." + functionName, completeFunc, new Array(url, kioskID, kStatus));		}		public static function completeFunc()		{			trace("KM: Monitor Complete");		}	}	}