package com.locusenergy.panels {		import flash.events.MouseEvent;	import flash.display.*;	import flash.filters.GlowFilter;	import caurina.transitions.Tweener;	import caurina.transitions.properties.FilterShortcuts;		public class HIWobject extends Sprite{				// Constants:		// Public Properties:		var sprite:Sprite;		var glowFilter:GlowFilter;		// Private Properties:			// Initialization:		public function HIWobject(sprite:Sprite) 		{ 			FilterShortcuts.init();			this.sprite = sprite;			this.addChild(this.sprite);			this.addEventListener(MouseEvent.CLICK, clickedEvent);		}		private function clickedEvent(me:MouseEvent)		{			highlight();		}		public function highlight()		{			glowFilter = new GlowFilter(0xdbff3d, 0.7, 20, 20, 2, 2, false, false);			//sprite.filters = [glowFilter];			Tweener.addTween(sprite, {_filter: glowFilter, time:1, transition: "easeOutBack"});		}		public function highlightIntense(t, firstDelay)		{			var currGlow = sprite.filters[0];			var intGlowFilter = new GlowFilter(0x48f5ff, 0.7, 50, 50, 2, 2, false, false);			//sprite.filters = [glowFilter];			Tweener.addTween(sprite, {_filter: intGlowFilter, delay:firstDelay, time:t/2});			Tweener.addTween(sprite, {_filter: glowFilter, delay: firstDelay + t/2, time:t/2});					}		public function unhighlight()		{			glowFilter = new GlowFilter(0xdbff3d, 0.0, 20, 20, 2, 2, false, false);			Tweener.addTween(sprite, {_filter: glowFilter, time:0.5});		}		public function cleanUp()		{			if (sprite.numChildren > 1)			{				while (sprite.numChildren > 1)				{					sprite.removeChildAt(1);				}			}			this.removeChild(sprite);			Tweener.removeAllTweens();		}		// Public Methods:		// Protected Methods:	}	}