package com.locusenergy.panels {		import flash.display.*;	import flash.events.*;		public class CurrentGenerationPanel extends Panel {				// Constants:		// Public Properties:		// Private Properties:		private var loadPanel:CurrentLoadGenPanel;		// Initialization:		public function CurrentGenerationPanel(connector, nodes='all') { 						loadPanel = new CurrentLoadGenPanel(connector, "CurrentGeneration", nodes);			loadPanel.addEventListener("DataLoaded",	dispatchLoaded, false, 0, true);			addChild(loadPanel);		}		override public function cleanUp()		{			if (loadPanel.hasEventListener("DataLoaded"))				loadPanel.removeEventListener("DataLoaded", dispatchLoaded);			removeChild(loadPanel);			loadPanel.cleanUp();		}		private function dispatchLoaded(e)		{			dispatchEvent(new Event("DataLoaded"));		}				// Public Methods:		// Protected Methods:	}	}