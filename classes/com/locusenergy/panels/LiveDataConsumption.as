package com.locusenergy.panels {		import flash.events.*;		public class LiveDataConsumption extends DataPanel{				// Constants:		// Public Properties:		// Private Properties:			// Initialization:		public function LiveDataConsumption(connector, nodes = "all") 		{			//getNetMeterConfig(connector.xml);			category = "Consumption";			loadChartFunc = loadBarChart;			componentName = "LiveData" + category;			paramArray["timeseries"] = type;			paramArray["nodes"] = nodes;			totalStr = "consumption";			this.connector = connector;			connector.getComponentData(componentName, loadChartData, paramArray);		}	}	}