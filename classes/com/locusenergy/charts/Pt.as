package com.locusenergy.charts {		import flash.display.Sprite;		public class Pt extends Sprite{				// Constants:		// Public Properties:		public var xVal:Number;		public var yVal:Number;		public var month;		public var year;		public var type;		public var bubbleColor;		// Private Properties:			// Initialization:		public function Pt(xVal, yVal) 		{			this.xVal = xVal;			this.yVal = yVal;			bubbleColor = 0;			type = 0;		}				public function highlightSelected()		{			trace("highlight ", xVal, yVal);		}		// Public Methods:		// Protected Methods:	}	}