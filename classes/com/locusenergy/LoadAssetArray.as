package com.locusenergy {	import com.locusenergy.AssetLoader;	import flash.display.Sprite;	import flash.events.Event;	import flash.events.EventDispatcher;	public class LoadAssetArray extends Sprite {				// Constants:		// Public Properties:		public  var PVImage:Sprite;		public  var ProjectInformationImages:Array;		public  var EnvBenefitsImages:Array;		// Private Properties:		private  var assetLoader:AssetLoader;		private  var imageArray:Array;		private var imageURLArray;		private  var fillArray:Array;		private var output;		// Initialization:		public function LoadAssetArray() { }			public function loadImageArray(imageURLArray, fillArray, output = null)		{			if (output != null)				this.output = output;			this.fillArray = fillArray;			this.imageURLArray = imageURLArray;			if (imageURLArray != null)				loadImage();			else				if (output != null) 				{					output.appendText(" - not loaded");					dispatchEvent(new Event("ArrayLoaded"));				}		}		private function loadImage()		{			if (imageURLArray.length > 0)			{				if (output != null) output.appendText("\n " + imageURLArray[0]);				assetLoader = new AssetLoader(imageURLArray[0], addImage);				assetLoader.addEventListener("ImageNotFound", nextImage, false, 0, true);				//if (output != null) output.appendText(imageURLArray.splice(0,1));				//else 				imageURLArray.splice(0,1);			}		}		private function addImage(asset)		{			fillArray.push(asset);			nextImage(null);		}		private function nextImage(e)		{			if (imageURLArray.length > 0)				loadImage();			else				dispatchEvent(new Event("ArrayLoaded"));					}	}	}